{
  "name": "zenless-club-remix-9688",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "node build.mjs",
    "dev": "run-s build:* && run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -i ./app/styles/index.css -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node --require ./node_modules/dotenv/config ./build/server.js",
    "typecheck": "tsc -b",
    "validate": "run-p lint typecheck"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.12",
    "@prisma/client": "^4.5.0",
    "@remix-run/express": "^1.7.3",
    "@remix-run/node": "^1.7.3",
    "@remix-run/react": "^1.7.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "accept-language-parser": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cuid": "^2.1.8",
    "dayjs": "^1.11.6",
    "express": "^4.18.2",
    "ioredis": "^5.2.3",
    "isbot": "^3.6.1",
    "jotai": "^1.8.6",
    "morgan": "^1.10.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "tiny-invariant": "^1.2.0",
    "unified": "^10.1.2",
    "use-intl": "^2.8.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^1.7.3",
    "@remix-run/eslint-config": "^1.7.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/accept-language-parser": "^1.5.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.7",
    "@types/node": "^18.11.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.11",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.12",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^6.0.4",
